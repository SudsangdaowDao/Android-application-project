<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.sudsangdoaw.sutvirtour.MainActivity"
    tools:showIn="@layout/app_bar_main">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton"
        android:elevation="10dp"
        android:layout_marginTop="180dp"
        android:layout_marginLeft="40dp"
        android:src="@drawable/btnmap"
        android:onClick="sendToMap"
        android:background="@null"
        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton1"
        android:elevation="10dp"
        android:src="@drawable/btncamera"
        android:layout_marginLeft="155dp"
        android:layout_marginTop="30dp"
        android:background="@null"
        />

    />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/camera_map"
        android:textColor="@color/map_text"
        android:textSize="15dp"
        android:textStyle="normal"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="405dp"
 />




</RelativeLayout>
