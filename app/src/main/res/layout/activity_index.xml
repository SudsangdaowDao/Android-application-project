<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#424242"
    tools:context="com.example.sudsangdoaw.sutvirtour.Index">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--SUT "
        android:textSize="30dp"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:id="@+id/textView4" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-Virtual guide in your hand-"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView5" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:textStyle="bold"
        android:text="Virtual "
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        android:id="@+id/textView3"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:textSize="30dp"
        android:text="Tour--"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView3"
        android:layout_toEndOf="@+id/textView3" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/video_index"
        android:background="@null"
        android:onClick="sendToVideoIntro"
        android:id="@+id/imageButton37"
        android:layout_alignTop="@+id/imageButton36"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/camera_index"
        android:background="@null"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="SensToCamera"
        android:id="@+id/imageButton35" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/map_index"
        android:background="@null"
        android:onClick="sendToMaps"
        android:id="@+id/imageButton36"
        android:layout_alignTop="@+id/imageButton35"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/information_index"
        android:background="@null"
        android:onClick="sendToInformation"
        android:id="@+id/imageButton38"
        android:layout_alignTop="@+id/imageButton39"
        android:layout_toLeftOf="@+id/imageButton37"
        android:layout_toStartOf="@+id/imageButton37" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/gallery_index"
        android:background="@null"
        android:onClick="sendToGallery"
        android:id="@+id/imageButton39"
        android:layout_below="@+id/imageButton36"
        android:layout_toRightOf="@+id/imageButton35"
        android:layout_toEndOf="@+id/imageButton35" />
    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/guide"
        android:layout_below="@+id/imageButton38"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5" />

